    "aws-sdk": "^2.546.0",
    "axios": "^0.18.1",
    "babel-loader": "^8.0.6",
    "chai": "^4.1.2",
    "cross-env": "^6.0.3",
    "ejs": "^2.7.1",
    "html-webpack-plugin": "^3.2.0",
    "mapbox-gl": "^1.4.0",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "react": "^16.10.2",
